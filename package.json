{
  "name": "observe",
  "displayName": "observe",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "observe.helloWorld",
    "observe.refresh"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "observe-sidebar-view",
          "title": "Observe",
          "icon": "media/telescope.svg"
        }
      ]
    },
    "views": {
      "observe-sidebar-view": [
        {
          "type": "webview",
          "id": "observe-sidebar",
          "name": "Observe",
          "icon": "media/telescope.svg",
          "contextualTitle": "Observe"
        }
      ]
    },
    "commands": [
      {
        "command": "observe.helloWorld",
        "title": "Hello World"
      },
      {
        "title": "Enable CodeLens",
        "command": "observe.enableCodeLens",
        "category": "Observe"
      },
      {
        "title": "Disable Codelens",
        "command": "observe.disableCodeLens",
        "category": "Observe"
      },
      {
        "title": "Refresh",
        "command": "observe.refresh",
        "category": "Observe"
      }
    ],
    "configuration": {
      "properties": {
        "observe.enableCodeLens": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "rollup -c --bundleConfigAsCjs && tsc -p ./",
    "watch": "concurrently \"rollup -c -w --bundleConfigAsCjs\" \"tsc -watch -p ./\"",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@bulatdashiev/svelte-slider": "^1.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@rollup/pluginutils": "^5.0.2",
    "@tsconfig/svelte": "^4.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/plotly.js": "^2.12.18",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "install": "^0.13.0",
    "mocha": "^10.2.0",
    "plotly.js-dist": "^2.21.0",
    "rollup": "^3.20.2",
    "rollup-plugin-css-only": "^4.3.0",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "svelte-range-slider-pips": "^2.1.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "rollup-plugin-svelte": "^6.1.1"
  }
}
